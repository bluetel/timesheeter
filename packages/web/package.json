    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "autoprefixer": "^10.4.20",
    "eslint-config-next": "^13.2.1",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.14",
